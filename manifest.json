{
  "manifest_version": 2,
  "name": "Similar Items search - demo widget",
  "description": "This extension...",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/jquery-2.2.1.min.js",
        "scripts/main.js"
      ],
      "matches": [
        "http://www.gap.com/browse/product.do*"
      ],
      "css": [
        "styles/main.css",
        "styles/spinner.css"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "styles/*.css",
    "scripts/*.js"
  ]
}
